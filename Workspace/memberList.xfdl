	objCF.init("popDept",0,0,600,400);
	objCF.set_openalign("center middle");
	objCF.set_formurl("Workspace::form_pop_id.xfdl");
	
	objCF.showModal( this.getOwnerFrame(), {}, this, "fn_callback_popId" );
