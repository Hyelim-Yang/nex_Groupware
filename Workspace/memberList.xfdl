    <Script type="xscript5.1"><![CDATA[include "lib_comm::grid_head_sort.xjs";

this.gds_stMember = nexacro.getApplication().gds_stMember;

this.grdList_onheadclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
