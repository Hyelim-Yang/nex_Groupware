this.fn_callback_popId = function(id, sRtn){
	var result = sRtn;
	if (sRtn==null)
	{
		nexacro.getApplication().gds_stMember.deleteRow(this.gds_stMember.rowposition);
	}
	else
	{
		this.Div00.form.Edit00.set_value(sRtn);
	}
}]]></Script>
